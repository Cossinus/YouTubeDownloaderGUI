<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:YouTubeDownloader.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="YouTubeDownloader.Views.EditorView"
             x:DataType="vm:EditorViewModel">
    
    <Grid RowDefinitions="*,*,*" ColumnDefinitions="*,*,*">
        <Label Content="{Binding Test}"/>
        <Button Grid.Row="1" Grid.Column="1" Command="{Binding TestButCommand}">
            <Label Content="Test"/>
        </Button>
        <Image Grid.Row="2" Grid.Column="2" Source="/Assets/background.png" Stretch="UniformToFill"/>
    </Grid>
</UserControl>
